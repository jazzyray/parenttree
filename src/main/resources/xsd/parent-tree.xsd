<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ontotext.com/parenttree/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="parenttree">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="tree" maxOccurs="1" minOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="leaf" maxOccurs="unbounded" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="children" maxOccurs="unbounded" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="leaf" maxOccurs="unbounded" minOccurs="0">
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute type="xs:anyURI" name="id" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="nodes" maxOccurs="1" minOccurs="1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="node" maxOccurs="unbounded" minOccurs="0">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:string">
                                            <xs:attribute type="xs:anyURI" name="id" use="required"/>
                                            <xs:attribute type="xs:string" name="preferredLabel" use="required"/>
                                            <xs:attribute type="xs:string" name="label" use="optional"/>
                                            <xs:attribute type="xs:string" name="preferredLabelTree" use="required"/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>